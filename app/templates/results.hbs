<h1>Results</h1>
<div class="flex-container">
  {{#each model as |search|}}
    {{#link-to 'details' search.id}}{{search.name}}{{/link-to}}
    Followers: {{search.followers.total}} <br>
    Popularity:{{search.popularity}} <br>
    Genres: {{search.genres}} <br>

    <img src="{{search.images.[0].url}}" alt="Image" width="300px" height="300px">
  {{/each}}
</div>
